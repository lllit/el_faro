{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.php",
            "use": "vercel-php@0.7.3"
        },
        {
            "src": "/Public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/page/(.*)",
            "dest": "/public/page/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/Public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/Public/js/$1"
        },
        {
            "src": "/Assets/imagenes/(.*)",
            "dest": "/Public/Assets/imagenes/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}