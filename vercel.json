{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "builds": [
        {
            "src": "/api/index.php",
            "use": "vercel-php@0.7.3"
        },
        {
            "src": "/style.css",
            "use": "@vercel/static"
        },
        {
            "src": "/main.js",
            "use": "@vercel/static"
        },
        {
            "src": "/imagenes/(.*)",
            "use": "@vercel/static"
        },
        {
            "src": "/audio/(.*)",
            "use": "@vercel/static"
        },
        {
            "src": "/paginas/(.*)",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/style.css", "dest": "/style.css" },
        { "src": "/main.js", "dest": "/main.js" },
        { "src": "/imagenes/(.*)", "dest": "/imagenes/$1" },
        { "src": "/audio/(.*)", "dest": "/audio/$1" },
        { "src": "/paginas/(.*)", "dest": "/paginas/$1" },
        { "src": "/app/(.*)", "dest": "/app/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}